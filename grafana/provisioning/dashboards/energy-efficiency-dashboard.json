        "orientation": "horizontal",
        "showValue": "auto",
        "stacking": "none",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 0
      },
      "pluginVersion": "9.5.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "# Efficiency stability: variance in power consumption over time\n# Lower variance = more stable configuration\nstddev_over_time(results_scenario_mean_power_watts[5m]) \n/ avg_over_time(results_scenario_mean_power_watts[5m])",
          "hide": false,
