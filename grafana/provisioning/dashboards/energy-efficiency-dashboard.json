  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "DS_PROMETHEUS"
      },
      "description": "Ranking of transcoding scenarios by energy efficiency score. Higher scores indicate better efficiency (more throughput per watt).",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "displayMode": "auto",
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "efficiency_score"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              },
              {
                "id": "unit",
                "value": "none"
              },
              {
                "id": "decimals",
                "value": 4
              },
              {
                "id": "custom.width",
                "value": 200
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "mean_power_watts"
            },
            "properties": [
              {
                "id": "unit",
                "value": "watt"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "total_energy_joules"
            },
            "properties": [
              {
                "id": "unit",
                "value": "joule"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "footer": {
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "efficiency_score"
          }
        ]
      },
      "pluginVersion": "9.5.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "DS_PROMETHEUS"
          },
          "editorMode": "code",
          "expr": "max by(scenario, output_ladder, encoder_type, streams) (results_scenario_efficiency_score{scenario!~\".*[Bb]aseline.*\", run_id=~\"$run_id\", output_ladder=~\"$output_ladder\", encoder_type=~\"$encoder_type\"})",
          "format": "table",
          "hide": false,
          "instant": true,
          "legendFormat": "__auto",
          "range": false,
          "refId": "Efficiency"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "DS_PROMETHEUS"
          },
          "editorMode": "code",
          "expr": "max by(scenario, output_ladder, encoder_type, streams) (results_scenario_mean_power_watts{scenario!~\".*[Bb]aseline.*\", run_id=~\"$run_id\", output_ladder=~\"$output_ladder\", encoder_type=~\"$encoder_type\"})",
          "format": "table",
          "hide": false,
          "instant": true,
          "legendFormat": "__auto",
          "range": false,
          "refId": "Power"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "DS_PROMETHEUS"
          },
          "editorMode": "code",
          "expr": "max by(scenario, output_ladder, encoder_type, streams) (results_scenario_total_energy_joules{scenario!~\".*[Bb]aseline.*\", run_id=~\"$run_id\", output_ladder=~\"$output_ladder\", encoder_type=~\"$encoder_type\"})",
          "format": "table",
          "hide": false,
          "instant": true,
          "legendFormat": "__auto",
          "range": false,
          "refId": "Energy"
        }
      ],
      "title": "Energy Efficiency Leaderboard",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true,
              "run_id": true
            },
            "indexByName": {
              "scenario": 0,
              "output_ladder": 1,
              "encoder_type": 2,
              "streams": 3,
              "Value #Power": 4,
              "Value #Energy": 5,
              "Value #Efficiency": 6
            },
            "renameByName": {
              "Value #Efficiency": "efficiency_score",
              "Value #Energy": "total_energy_joules",
              "Value #Power": "mean_power_watts",
              "encoder_type": "encoder",
              "output_ladder": "output_ladder",
              "scenario": "scenario",
              "streams": "streams"
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "fields": {},
            "sort": [
              {
                "desc": true,
                "field": "efficiency_score"
              }
            ]
          }
        }
      ],
      "type": "table"
    },
